<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.blizzard.com/wow/ui/
..\Blizzard_SharedXML\UI.xsd">
	<Frame name="ArachnophobiaTemplate" inherits="SettingsCheckboxControlTemplate" mixin="ArachnophobiaMixin" virtual="true">
		<Frames>
			<Frame parentKey="SubTextContainer" setAllPoints="true" hyperlinksEnabled="true" enableMouse="true">
				<Layers>
					<Layer>
						<FontString parentKey="SubText" inherits="GameFontNormal" justifyH="LEFT" text="ARACHNOPHOBIA_MODE_SUBTEXT">
							<Size x="350" y="0"/>
						</FontString>
					</Layer>
				</Layers>
				<Scripts>
					<OnHyperlinkClick function="GetURLIndexAndLoadURL" />
				</Scripts>
			</Frame>
		</Frames>
	</Frame>
</Ui>
